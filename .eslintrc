{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "no-alert": 0,
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "no-undef": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "max-classes-per-file": 0,
    "no-underscore-dangle": 0
  }
}
